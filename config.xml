<?xml version="1.0" encoding="UTF-8"?>
<widget xmlns="http://www.w3.org/ns/widgets" xmlns:gap="http://phonegap.com/ns/1.0" id="edu.uab.business.studentapp" versionCode="1" version="0.0.1">
    <!--versionCode is optional and Android only -->
    <name>Module 1</name>
    <description>Mobile App Module 1</description>
    <author>Jared O'Neal</author>
    <preference name="phonegap-version" value="cli-6.0.1" />
    <plugin name="cordova-plugin-whitelist" source="npm" />
    <access origin="*" />
    <plugin name="cordova-plugin-geolocation" spec="2.4.3" source="npm" />
    <plugin name="cordova-plugin-splashscreen" source="npm" />
    <plugin name="cordova-plugin-camera" source="npm" spec="2.4.1"/>
    <plugin name="cordova-plugin-contacts" source="npm" spec="2.3.1" />
    <icon src="icon.png"/>
      <splash src="splash.png" />
    <splash src="splash.png" platform="android" qualifier="anydpi" />
<splash src="splash.png" platform="android" qualifier="mdpi" />
<splash src="splash.png" platform="android" qualifier="hdpi" />
<splash src="splash.png" platform="android" qualifier="xhdpi" />
<splash src="splash.png" platform="android" qualifier="fr-xhdpi" />
<splash src="splash.png" platform="android" qualifier="port-xxhdpi" />
<splash src="splash.png" platform="android" qualifier="land-xxhdpi" />
<splash src="splash.png" platform="android" qualifier="xxxhdpi" />
<icon src="icon-ios.png" platform="ios" width="180" height="180" />
<icon platform="winphone" width="150" height="150" src="icon-150.png" />
<icon src="icon-150.png" platform="winphone" />

</widget>